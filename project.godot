config_version=5

[application]

config/name="TKMapper"
run/main_scene="res://TKMapper.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.png"

[autoload]

Database="*res://Resources/Database.gd"
MapperState="*res://Resources/MapperState.gd"
Resources="*res://Resources/Resources.gd"
Debug="*res://Resources/Debug.gd"

[display]

window/size/viewport_width=1440
window/size/viewport_height=900
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray()

[importer_defaults]

font_data_bmfont={
"compress": true,
"fallbacks": [],
"scaling_mode": 2
}

